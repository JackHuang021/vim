{
        "files.exclude": {
                "**/node_modules": true,
                "**/bower_components": true,
                "**/*.code-search": true,
                "**/*.o": true,
                "**/*.ko": true,
                "**/*.dtb": true,
                "**/*.dtbo": true,
                "**/*.dtb.S": true,
